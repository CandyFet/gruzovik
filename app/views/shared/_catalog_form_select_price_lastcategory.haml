- if lastcategories.any?
  = select_tag :q_lastcategory, options_from_collection_for_select(lastcategories, 'id', 'name', params[:q_lastcategory]), include_blank: 'Все категории', class: 'styler', data: {placeholder: 'КАТЕГОРИЯ'}
- else
  = select_tag :q_lastcategory, options_from_collection_for_select(lastcategories, 'id', 'name'), include_blank: true, class: 'styler', data: {placeholder: 'КАТЕГОРИЯ'}, disabled: true

